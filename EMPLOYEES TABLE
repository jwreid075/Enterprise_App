CREATE TABLE EMPLOYEES (emp_num NUMBER(38), FIRST_NAME VARCHAR2(20), LAST_NAME VARCHAR2(20), DELIVERY_ZIP_CODE NUMBER(10), STREET_ADDRESS VARCHAR2(50), CITY VARCHAR2(20), STATE_ABBR VARCHAR2(2), HOME_ZIP_CODE VARCHAR(10));

