
CREATE TABLE food (FOOD_ID NUMBER(38), FOOD_NAME VARCHAR(50), DESCRIPTION VARCHAR(200), AMOUNT NUMBER(38));

CREATE SEQUENCE FOOD_ID START WITH 1000 MINVALUE 1000 INCREMENT BY 1 CACHE 100;

